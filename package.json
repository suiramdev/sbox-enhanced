{
    "name": "sbox-enhanced",
    "displayName": "S&box Enhanced",
    "description": "S&box language support for VS Code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "snippets": [
			{
				"language": "cs",
				"path": "./snippets/custom.json"
			}
		],
        "languages": [{
            "id": "cs",
            "aliases": ["C#", "cs"],
            "extensions": [".cs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cs",
            "scopeName": "source.cs",
            "path": "./syntaxes/csharp.tmLanguage"
        }]
    }
}